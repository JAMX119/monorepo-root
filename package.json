{
  "name": "microservices-monorepo",
  "private": true,
  "workspaces": [
    "apps/*",
    "configs/*",
    "packages/*",
    "scripts/*",
    "services/*"
  ],
  "scripts": {
    "dev": "turbo run dev --parallel",
    "build": "turbo run build",
    "test": "turbo run test",
    "lint": "turbo run lint",
    "format": "prettier --write '**/*.{ts,tsx,js,jsx,json,md}'"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.5.8"
  },
  "packageManager": "pnpm@10.18.2",
  "engines": {
    "node": ">=18"
  },
  "dependencies": {
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "express": "^4.19.2"
  },
  "pnpm": {
    "overrides": {
      "react": "^19.1.0",
      "react-dom": "^19.1.0",
      "express": "^4.19.2",
      "typescript": "^5.4.5"
    }
  }
}